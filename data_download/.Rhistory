library(jsonlite)
library(tidyverse)
json_file_path <- "uuid:ae767057-435d-11dd-b505-00145e5790ea_prehled.json"
data <- fromJSON(json_file_path)
# Počet ročníků
pocet_rocniku <- nrow(data)
# Počet issues v ročnících
data$number_of_issues <- sapply(data$issues, nrow)
# Počet stran v číslech
total_pages_per_volume <- numeric(nrow(data))
for (i in 1:nrow(data)) {
volume_total_pages <- 0
for (j in 1:nrow(data$issues[[i]])) {
pages_in_single_issue <- nrow(data$issues[[i]]$pages[[j]])
volume_total_pages <- volume_total_pages + pages_in_single_issue
}
total_pages_per_volume[i] <- volume_total_pages
}
data$total_pages_per_volume <- total_pages_per_volume
library(knitr)
# Optionally, select specific columns or format the data frame as needed
data_table <- data %>% select(volume_no, volume_year, numer_of_issues, total_pages_per_volume)
library(knitr)
library(dplyr)
# Select specific columns
selected_data <- data %>%
select(volume_no, volume_year, number_of_issues, total_pages_per_volume)
# Create the table
kable(selected_data, caption = "Key Metrics of Volume Data")
