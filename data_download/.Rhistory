library(jsonlite)
library(tidyverse)
json_file_path <- "uuid:8c24dbf9-8f66-4332-8c76-ba5b653c7baf_prehled.json"
data <- fromJSON(json_file_path)
# Flatten the data
flat_data <- data %>%
unnest(issues) %>%
unnest(pages) %>%
unnest(page_uuid, .sep = "_") %>%
unnest(TEXT_OCR_exists, .sep = "_") %>%
unnest(ALTO_exists, .sep = "_")
# Počet ročníků
pocet_rocniku <- nrow(data)
# Počet issues v ročnících
data$number_of_issues <- sapply(data$issues, nrow)
# Počet stran v číslech
total_pages_per_volume <- numeric(nrow(data))
for (i in 1:nrow(data)) {
volume_total_pages <- 0
for (j in 1:nrow(data$issues[[i]])) {
pages_in_single_issue <- nrow(data$issues[[i]]$pages[[j]])
volume_total_pages <- volume_total_pages + pages_in_single_issue
}
total_pages_per_volume[i] <- volume_total_pages
}
data$total_pages_per_volume <- total_pages_per_volume
library(knitr)
library(dplyr)
# Select specific columns
selected_data <- data %>%
select(volume_no, volume_year, number_of_issues, total_pages_per_volume)
mean_TEXT_OCR <- flat_data %>%
group_by(volume_uuid) %>%
summarise(mean_TEXT_OCR = mean(TEXT_OCR_exists, na.rm = TRUE))
mean_ALTO <- flat_data %>%
group_by(volume_uuid) %>%
summarise(mean_ALTO = mean(ALTO_exists, na.rm = TRUE))
# Create the table
kable(c(selected_data, mean_TEXT_OCR, mean_ALTO), caption = "Key Metrics of Volume Data")
